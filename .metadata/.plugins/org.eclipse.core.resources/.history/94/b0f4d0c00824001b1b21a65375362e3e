			<![CDATA[SELECT 
	cr.numero_credit,
	cr.status_credit,
	 cr.date_expire_credit,
	 cr.amount_net,
	 cr.amount_payement,
	 cli.first_name,
	 cli.last_name,
	 cli.phone,
	 cli.numero as numcli
FROM credits cr
INNER JOIN clients cli
	on cr.client_id = cli.id]]>
